---
import MovilNav from "./header/headerMovil.astro";
import DesktopNav from "./header/headerDesktop.astro";
import Hero from "./index/hero.astro";

const { PrintHero } = Astro.props;
---

<header>
    <MovilNav />
    <DesktopNav />
    {PrintHero ? <Hero /> : <h1>Hola</h1>}
</header>
<style>
    header {
        height: fit-content;
        width: 100%;
    }
</style>
